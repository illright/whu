{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "whu",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "close": true,
        "setAlwaysOnTop": true,
        "setFullscreen": true,
        "setClosable": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.illright.whu",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [],
    "systemTray": {
      "iconPath": "icons/tray-template.png",
      "iconAsTemplate": true
    }
  }
}
